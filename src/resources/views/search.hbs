<div class="container">
  <h1>{{title}}</h1>
  <p>Web bán truyện được ưa thích nhất</p>
  <div class="book-information">
    <h2 style="color: orange;">Kết quả tìm kiếm cho: PHAI DIEN</h2>
    <div class="row">
      <div class="col-2">
        <p>Sắp xếp theo</p>
      </div>
      <div class="col-10">
        <select class="form-select" style="width: 250px;">
          <option selected>Select--</option>
          <option value="1">Thời gian</option>
          <option value="2">Lượt mua</option>
          <option value="3">Giá tiền</option>
        </select>
      </div>
    </div>

    <div class="mt-4">
      <div class="row">
        {{#each searchtitle}}
        <div class="col-sm-6 col-lg-4">
          <div class="card" style=" height:100%">
            <img src="{{this.HINHANH}}" class="card-img-top" alt="conan_84" />

            <div class="card-body">
              <a href="/{{this.masach}}">
                <div class="card-title">
                  {{this.tensach}}
                </div>
              </a>
              <p class="card-text">{{this.MOTA}}</p>
            </div>
          </div>
        </div>
        {{/each}}

      </div>
    </div>




